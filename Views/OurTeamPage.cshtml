@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.OurTeamPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_LayoutWithBreadcrumb.cshtml";
	ViewData["PageTitle"] = "Our Team";
	var selection = Umbraco.Content(Guid.Parse("32d4b0de-c378-4897-833d-5df91bf562bb"))
        .ChildrenOfType("ourTeamDetailPage")
        .Where(x => x.IsVisible())
        .OrderBy(x => x.CreateDate);
}


<div class="container">
    <div class="d-flex justify-content-center my-5">
        <ul class="list-inline text-center">
        	@foreach (var item in selection)
        	{
        		<li class="my-1">
        			<a href="@item.Url()">@item.Name()</a>
        		</li>
        	}
        </ul>
    </div>
</div>

